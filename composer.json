{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^2.4",
        "drupal/ajax_loader": "^1.4",
        "drupal/anonymous_login": "^2.0",
        "drupal/audiofield": "^1.10",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_class": "^1.3",
        "drupal/bootstrap_library": "^1.14",
        "drupal/bs_tour": "^1.5",
        "drupal/chosen": "^3.0",
        "drupal/components": "^1.1",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/content_lock": "^2.2",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/core-vendor-hardening": "^8.8",
        "drupal/devel": "^4.0",
        "drupal/dropzonejs": "^2.3",
        "drupal/entity": "^1.2",
        "drupal/entity_browser": "^2.5",
        "drupal/entityreference_filter": "^1.2",
        "drupal/exclude_node_title": "^1.1",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_ex": "^1.0@alpha",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.1",
        "drupal/flag": "^4.0@beta",
        "drupal/fontawesome": "^2.17",
        "drupal/fontawesome_menu_icons": "^1.9",
        "drupal/front": "^1.0@beta",
        "drupal/geolocation": "^3.2",
        "drupal/http_auth": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_plugin_views": "^1.1",
        "drupal/libraries": "^3.0@alpha",
        "drupal/loading_bar": "^1.0@beta",
        "drupal/login_history": "^1.0",
        "drupal/mail_entity_queue": "^1.0@alpha",
        "drupal/mailsystem": "^4.3",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.6",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/menu_link_attributes": "^1.1",
        "drupal/menu_token": "^1.0@alpha",
        "drupal/paragraphs": "^1.12",
        "drupal/profile": "^1.2",
        "drupal/queue_ui": "^2.2",
        "drupal/radix_layouts": "^4.1",
        "drupal/redirect_after_login": "^2.6",
        "drupal/registration_role": "^1.1",
        "drupal/simple_popup_views": "^1.0@beta",
        "drupal/simplelogin": "^6.0",
        "drupal/smart_trim": "^1.3",
        "drupal/smtp": "^1.0",
        "drupal/swiftmailer": "^2.0@beta",
        "drupal/tamper": "^1.0@alpha",
        "drupal/token": "^1.9",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/user_view_mode": "^1.0",
        "drupal/vapn": "^1.5",
        "drupal/views_entity_form_field": "^1.0@beta",
        "drupal/views_flipped_table": "^1.4",
        "drupal/views_layout": "^1.2",
        "drupal/views_secondary_row": "^1.0",
        "drupal/webform": "^5.22",
        "drupal/workbench": "^1.3",
        "drupal/workbench_access": "^1.0@beta",
        "drupal/yaml_editor": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
